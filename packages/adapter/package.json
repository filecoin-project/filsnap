{
  "name": "filsnap-adapter",
  "type": "module",
  "version": "3.3.9",
  "description": "Adapter for installing Filsnap",
  "author": "Protocol Labs",
  "license": "(Apache-2.0 AND MIT)",
  "homepage": "https://github.com/filecoin-project/filsnap/tree/master/packages/adapter",
  "repository": {
    "type": "git",
    "url": "https://github.com/filecoin-project/filsnap.git",
    "directory": "packages/adapter"
  },
  "bugs": {
    "url": "https://github.com/filecoin-project/filsnap/issues"
  },
  "keywords": [
    "filecoin",
    "metamask",
    "snap",
    "dapp"
  ],
  "files": [
    "src",
    "dist/src/**/*.js",
    "dist/src/**/*.d.ts",
    "dist/src/**/*.d.ts.map"
  ],
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./install": {
      "types": "./dist/src/install.d.ts",
      "default": "./dist/src/install.js"
    }
  },
  "typesVersions": {
    "*": {
      "install": [
        "./dist/src/install"
      ]
    }
  },
  "scripts": {
    "lint": "tsc --build && biome check --no-errors-on-unmatched --files-ignore-unknown=true",
    "build": "wireit",
    "test": "pnpm run build && playwright test"
  },
  "wireit": {
    "build": {
      "command": "tsc --build --pretty",
      "clean": "if-file-deleted",
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": [
        "dist/**"
      ]
    }
  },
  "dependencies": {
    "iso-base": "^4.2.1",
    "semver": "^7.7.3"
  },
  "devDependencies": {
    "@metamask/snaps-sdk": "10.0.0",
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.10.1",
    "@types/semver": "^7.7.1",
    "filsnap": "workspace:^",
    "metamask-testing-tools": "^2.2.9",
    "type-fest": "^5.2.0",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "iso-filecoin": "7.x"
  },
  "publishConfig": {
    "provenance": true
  },
  "depcheck": {
    "specials": [
      "bin"
    ],
    "ignorePatterns": [
      "dist",
      ".wireit"
    ],
    "ignores": [
      "@types/*",
      "assert",
      "playwright-test"
    ]
  }
}
